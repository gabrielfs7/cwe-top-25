{
  "description": "Examples in PHP for 2011 CWE/SANS Top 25 Most Dangerous Software Errors. Referenced to http://cwe.mitre.org/top25/",
  "type" : "project",
  "authors": [
    {
      "name": "Gabriel Felipe Soares",
      "name": "gabrielfs7@gmail.com",
      "role": "Developer"
    }
  ],
  "require" : {
    "php": ">=7.0",
    "symfony/dependency-injection" : "v3.3.9",
    "symfony/http-kernel" : "v3.3.9",
    "symfony/templating" : "v3.3.9",
    "symfony/routing" : "v3.3.9",
    "twig/twig" : "~2.4.4",
    "twig/extensions" : "v1.5.1",
    "doctrine/migrations" : "1.5.0",
    "doctrine/orm" : "v2.5.11"
  },
  "require-dev": {
    "phpunit/phpunit": "6.3.1",
    "squizlabs/php_codesniffer" : "3.1.0",
    "phpmd/phpmd": "2.6.0",
    "phpmetrics/phpmetrics" : "v2.3.0",
    "phpbench/phpbench": "dev-master@dev",
    "object-calisthenics/phpcs-calisthenics-rules": "v3.0.1"
  },
  "autoload" : {
    "psr-4" : {
      "GSoares\\CweTop25\\" : "src"
    }
  },
  "autoload-dev" : {
    "psr-4" : {
      "GSoares\\CweTop25\\" : [
        "src/CweTop25",
        "test"
      ]
    }
  },
  "config": {
    "bin-dir" : "bin"
  }
}